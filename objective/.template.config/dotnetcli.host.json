{
    "$schema": "http://json.schemastore.org/dotnetcli.host",
    "symbolInfo": {
        "exerciseNumber": {
            "longName": "exercise-number",
            "shortName": "number"
        },
        "title": {
            "type": "parameter",
            "defaultValue": "Course long title (ABC101)",
            "replaces": "{title}"
        },
        "titleShort": {
            "type": "parameter",
            "defaultValue": "Course title",
            "replaces": "{titleShort}"
        },
        "description": {
            "type": "parameter",
            "defaultValue": "This course is going to be amazing. You will learn all sorts of wonderful things.",
            "replaces": "{description}"
        },
        "youTubeVideoId": {
            "type": "parameter",
            "defaultValue": "SrkbPinx3Zo",
            "replaces": "{youTubeVideoId}"
        }
    }
}